{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "F:\\coding"
      ]
    },
    "secret-network": {
      "command": "cmd",
      "args": [
        "/c",
        "cd /d F:\\coding\\secret-network-mcp && node dist\\server.js"
      ],
      "env": {
        "NODE_ENV": "production",
        "NETWORK": "mainnet",
        "LOG_LEVEL": "info",
        "MCP_MODE": "true",
        "SKIP_NETWORK_CHECK": "true",
        "ALLOW_OFFLINE_MODE": "true",
        "NETWORK_TIMEOUT": "1000"
      }
    }
  }
}